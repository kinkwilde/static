{
	"name": "static",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prettier:format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prettier:lint": "prettier --ignore-path .gitignore --check -- plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"svelte:check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"svelte:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"tina:dev": "npx tinacms dev -c 'npm run vite:dev'",
		"vite:build": "vite build",
		"vite:dev": "vite dev",
		"vite:preview": "vite preview"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.10.6",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1"
	},
	"type": "module",
	"dependencies": {
		"@tinacms/cli": "^1.0.9",
		"tinacms": "^1.1.5"
	}
}
